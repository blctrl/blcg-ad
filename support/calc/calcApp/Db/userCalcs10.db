record(bo, "$(P)userCalcEnable") {
  field(DESC, "userCalc Enable")
  field(ZNAM, "Disable")
  field(ONAM, "Enable")
  field(UDF, "0")
  field(STAT, "NO_ALARM")
  field(SEVR, "NO_ALARM")
}
record(bo, "$(P)EnableUserCalcs") {
  field(OMSL, "closed_loop")
  field(DOL, "1")
  field(OUT, "$(P)userCalcEnable.VAL  PP MS")
}
record(bo, "$(P)DisableUserCalcs") {
  field(OMSL, "closed_loop")
  field(OUT, "$(P)userCalcEnable.VAL  PP MS")
}

record(bo, "$(P)userCalc1Enable") {
  field(DESC, "userCalc 1 Enable")
  field(ZNAM, "D")
  field(ONAM, "E")
  field(UDF, "0")
  field(STAT, "NO_ALARM")
  field(SEVR, "NO_ALARM")
  field(DOL, "1")
}
record(calc, "$(P)userCalc1EnableCalc") {
	field(INPA, "$(P)userCalc1Enable CP")
	field(INPB, "$(P)userCalcEnable CP")
	field(INPC, "$(P)userCalcGlobalEnable CP")
	field(CALC, "a+b+c==3")
}
record(swait, "$(P)userCalc1") {
  field(DESC, "userCalc 1")
  field(DISV, "0")
  field(SDIS, "$(P)userCalcEnable.VAL  CA MS")
  field(PREC, "5")
}

