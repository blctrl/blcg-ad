record(bo, "$(P)userArrayCalcEnable") {
  field(DESC, "userACalc enable")
  field(ZNAM, "Disable")
  field(ONAM, "Enable")
  field(UDF, "0")
  field(STAT, "NO_ALARM")
  field(SEVR, "NO_ALARM")
}
record(bo, "$(P)EnableArrayCalcs") {
  field(OMSL, "closed_loop")
  field(DOL, "1")
  field(OUT, "$(P)userArrayCalcEnable.VAL  PP MS")
}
record(bo, "$(P)DisableArrayCalcs") {
  field(OMSL, "closed_loop")
  field(OUT, "$(P)userArrayCalcEnable.VAL  PP MS")
  field(DOL, "0")
}

record(bo, "$(P)userArrayCalc1Enable") {
  field(DESC, "userArrayCalc 1 Enable")
  field(ZNAM, "D")
  field(ONAM, "E")
  field(UDF, "0")
  field(STAT, "NO_ALARM")
  field(SEVR, "NO_ALARM")
  field(DOL, "1")
}
record(calc, "$(P)userArrayCalc1EnableCalc") {
	field(INPA, "$(P)userArrayCalc1Enable CP")
	field(INPB, "$(P)userArrayCalcEnable CP")
	field(INPC, "$(P)userCalcGlobalEnable CP")
	field(CALC, "a+b+c==3")
}
record(acalcout, "$(P)userArrayCalc1") {
  field(DESC, "userArrayCalc1")
  field(DISV, "0")
  field(SDIS, "$(P)userArrayCalcEnable.VAL  CA MS")
  field(PREC, "5")
  field(NELM, "$(N)")
  field(NUSE, "$(N)")
}

